<!-- See documentation at http://doc.nuxeo.com/x/HgIz -->
<package type="addon" name="nuxeo-mqueues" version="@VERSION@">
  <title>Nuxeo MQueues importer</title>
  <description>
    <p>MQueues provides producer/consumer and computation patterns.</p>
    <p>Producer/consumer pattern is used to impl efficient document importer.</p>
    <p>Computation pattern is used to impl an experimental WorkManager.</p>
  </description>
  <home-page></home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <!-- <hotreload-support>false</hotreload-support> -->
  <require-terms-and-conditions-acceptance>false</require-terms-and-conditions-acceptance>
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>proto</production-state>
  <supported>false</supported>
  <platforms>
    <platform>server-@DISTRIB_VERSION@</platform>
    <platform>server-@DISTRIB_VERSION@-*</platform>
  </platforms>
  <license>Apache 2.0</license>
  <license-url>http://www.apache.org/licenses/LICENSE-2.0</license-url>
  <visibility>DEV</visibility>
</package>
